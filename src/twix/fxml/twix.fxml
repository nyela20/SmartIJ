                  <Button fx:id="buttonSearch" mnemonicParsing="false" onAction="#search" prefHeight="26.0" prefWidth="72.0" text="search" visible="false" />
               </graphic>
            </Label>
            <Label fx:id="levelname" text="Level actuel : " />
         </children>
      </VBox>
   </left>
</BorderPane>

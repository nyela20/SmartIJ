   <left>
      <VBox prefHeight="400.0" prefWidth="387.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="filename" contentDisplay="TOP" prefHeight="26.0" prefWidth="367.0" text="Choisissez un fichier ...">
